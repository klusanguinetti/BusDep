<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="BusDep.Entity.Entrenador, BusDep.Entity" table="Entrenador" lazy="true">
    <id name="Id" column="EntrenadorId" type="System.Int64"  >
      <generator class="native" />
    </id>
    <property name="FotoRostro" column="FotoRostro" type="System.String" length="400" />
    <property name="Perfil" column="Perfil" type="System.String" length="200" />
  </class>
</hibernate-mapping>
